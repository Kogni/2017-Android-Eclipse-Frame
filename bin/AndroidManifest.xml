<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pynting.androidframe_c" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-sdk android:minSdkVersion="17" />

    <supports-screens
        android:compatibleWidthLimitDp="480"
        android:requiresSmallestWidthDp="320" />

    <application
        android:hardwareAccelerated="true"
        android:icon="@drawable/app_sample_code"
        android:label="@string/activity_sample_code" >
        <activity android:name="com.pynting.androidframe_c.Support13Demos2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pack_generic.Act_MainGUI">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="com.pynting.androidframe_c.SUPPORT13_SAMPLE_CODE" />
            </intent-filter>
        </activity>
    </application>

</manifest>